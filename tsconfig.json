{
    "compilerOptions": {
        "target": "es2015",
        "module": "commonjs",
        "sourceMap": false,
        "noImplicitAny": false,
        "jsx": "preserve",
        "baseUrl": "./src",
        "rootDir": "./src",
        "allowJs": true,
        "moduleResolution": "node",
        "strictNullChecks": false,
        "outDir": "./lib",
        "declaration": false
    },
  "include": [
      "src/**/*"
  ],
  "exclude": [
      "**/*.spec.ts",
      ".history/**/*"
  ]
}